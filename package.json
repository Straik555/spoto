{
  "name": "spoto-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "prod": "npm run build && npm run start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build && next export",
    "build:export": "next build && next export",
    "postbuild:export": "npx cap sync",
    "open:ios": "npx cap open ios",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "prettier-fix": "prettier --write .",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:clearCache": "npm run jest --clearCache",
    "test:e2e:run": "percy exec -c ./tests/e2e/config/percy.mobile.json -- cypress run --config-file=./tests/e2e/config/cypress.mobile.json",
    "test:e2e:server:run": "cross-env NODE_ENV=test start-server-and-test dev 3000 test:e2e:run",
    "test:e2e:open": "cypress open --config-file=./tests/e2e/config/cypress.mobile.json",
    "test:e2e:server:open": "cross-env NODE_ENV=test start-server-and-test dev 3000 test:e2e:open"
  },
